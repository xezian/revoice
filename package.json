{
  "name": "revoice",
  "version": "1.0.0",
  "description": "A game that involves reversing vocal audio clips",
  "main": "index.js",
  "repository": "git@github.com:xezian/revoice.git",
  "author": "xezian <xezian@icloud.com>",
  "license": "MIT",
  "engines": {
    "node": "10.11.0",
    "yarn": "1.10.1",
    "npm": "5.3.0"
  },
  "scripts": {
    "install": "cd client && yarn",
    "dev": "concurrently \"cd client && yarn start\" \"yarn start:dev\"",
    "start": "node server",
    "start:dev": "nodemon server",
    "lint": "eslint .",
    "test": "yarn test:server; yarn test:client",
    "test:client": "cd client; yarn test;",
    "test:server": "mocha --timeout 10000 --exit",
    "heroku-postbuild": "cd client && yarn run build"
  },
  "dependencies": {
    "aws-sdk": "^2.257.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chai-http": "^4.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "nekodb": "^2.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.5"
  }
}
